{
  "name": "bnow",
  "description": "Birmingham Now Project",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NEXT_BUILD=true node dist/server.js",
    "build": "cross-env NODE_ENV=production npm run build:payload && npm run build:server && npm run build:next",
    "build-green": "cross-env NODE_ENV=production PORT=3001 npm run build:payload && npm run build:server && npm run build:next",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NODE_ENV=production PORT=3000 node dist/server.js",
    "serve-green": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NODE_ENV=production PORT=3001 node dist/server.js",
    "eject": "yarn remove next react react-dom @next/eslint-plugin-next && ts-node eject.ts",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,ttf,woff,woff2,eot,svg,jpg,png,js}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:graphQLSchema",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload"
  },
  "dependencies": {
    "@mapbox/search-js-react": "1.0.0-beta.22",
    "@payloadcms/bundler-webpack": "^1.0.7",
    "@payloadcms/db-mongodb": "^1.7.2",
    "@payloadcms/plugin-cloud": "^3.0.2",
    "@payloadcms/plugin-nested-docs": "^1.0.12",
    "@payloadcms/plugin-redirects": "^1.0.2",
    "@payloadcms/plugin-seo": "^2.3.2",
    "@payloadcms/richtext-slate": "^1.5.2",
    "@types/react-plyr": "^2.1.4",
    "altcha": "^1.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "escape-html": "^1.0.3",
    "express": "^4.20.0",
    "fs": "0.0.1-security",
    "lodash": "^4.17.21",
    "mapbox-gl": "^3.6.0",
    "music-metadata": "^10.5.0",
    "next": "14.2.9",
    "next-themes": "^0.4.3",
    "payload": "^2.28.0",
    "payload-admin-bar": "^1.0.6",
    "plyr-react": "^5.3.0",
    "qs": "6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-h5-audio-player": "^3.9.3",
    "react-hook-form": "7.53.0",
    "react-icons": "^5.3.0",
    "react-map-gl": "^7.1.7",
    "react-router-dom": "6.26.2",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.9",
    "@payloadcms/eslint-config": "^1.1.1",
    "@swc/core": "1.7.24",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/node": "22.5.4",
    "@types/qs": "^6.9.15",
    "@types/react": "18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "autoprefixer": "^10.4.20",
    "copyfiles": "^2.4.1",
    "daisyui": "^4.12.10",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "file-loader": "^6.2.0",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "slate": "0.103.0",
    "tailwindcss": "^3.4.10",
    "ts-node": "10.9.2",
    "typescript": "^5.6.2"
  }
}
